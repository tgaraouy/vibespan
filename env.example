# Vibespan.ai Environment Variables

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/vibespan

# Redis
REDIS_URL=redis://localhost:6379

# JWT Secret
JWT_SECRET_KEY=your-super-secret-jwt-key-here

# Encryption
ENCRYPTION_KEY=your-32-byte-encryption-key-here

# LLM APIs
OPENAI_API_KEY=sk-...
ANTHROPIC_API_KEY=sk-ant-...

# WHOOP v2
WHOOP_CLIENT_ID=your-whoop-client-id
WHOOP_CLIENT_SECRET=your-whoop-client-secret
WHOOP_WEBHOOK_SECRET=your-whoop-webhook-secret
WHOOP_V2_BASE_URL=https://api.prod.whoop.com

# Apple Health
APPLE_HEALTH_WEBHOOK_SECRET=your-apple-webhook-secret

# Monitoring
SENTRY_DSN=your-sentry-dsn

# Environment
ENVIRONMENT=development
DEBUG=true
